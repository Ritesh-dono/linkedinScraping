{
    "manifest_version": 3,
    "name": "LinkedIn Scraper",
    "version": "1.0",
    "permissions": [
      "tabs",
      "activeTab",
      "scripting",
      "storage"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "host_permissions": [
      "*://*.linkedin.com/*"
    ]
  }
  